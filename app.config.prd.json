{
  "name": "s3-browser-prd",
  "script": "node_modules/next/dist/bin/next",
  "args": "start --port 3002",
  "instances": 1,
  "exec_mode": "fork",
  "error_file": "./logs/s3-browser-prd-error.log",
  "out_file": "./logs/s3-browser-prd-out.log",
  "log_date_format": "YYYY-MM-DD HH:mm:ss Z",
  "merge_logs": true,
  "env": {
    "NODE_ENV": "production",
    "PORT": "3002",
    "NEXT_PUBLIC_ENV": "production"
  },
  "env_production": {
    "NODE_ENV": "production"
  }
}

